$(->
  $("body").prepend("<div id='fb-root'></div>")

  $.ajax
    url: '//connect.facebook.net/en_US/all.js'
    dataType: 'script'
    cache: true

  window.fbAsyncInit = ->
    FB.init({
      appId: '<%= Studium::Application.config.facebook_app_id %>', 
      cookie: true,
      status: true,
      xfbml: true
    })

    $("#facepile").html("<fb:facepile></fb:facepile>")
)
